<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="create_table_order_auto_transport" author="ivan">

        <createTable tableName="order_auto_transport">
            <column name="order_id" type="int"/>
            <column name="auto_transport_id" type="int"/>
        </createTable>
    </changeSet>

    <changeSet id="add-foreign-key-constraint-to-order_auto_transport1" author="ivan">
        <addForeignKeyConstraint
                baseColumnNames="order_id"
                baseTableName="order_auto_transport"
                baseTableCatalogName="concretebelarus"
                constraintName="fk_order_id_oat"
                onUpdate="CASCADE"
                onDelete="NO ACTION"
                referencedColumnNames="id"
                referencedTableName="orders"
                referencedTableCatalogName="concretebelarus"
                referencedTableSchemaName="concretebelarus"
        />
    </changeSet>
    <changeSet id="add-foreign-key-constraint-to-order_auto_transport2" author="ivan">
        <addForeignKeyConstraint
                baseColumnNames="auto_transport_id"
                baseTableName="order_auto_transport"
                baseTableCatalogName="concretebelarus"
                constraintName="fk_grade_auto_transport_oat"
                onUpdate="CASCADE"
                onDelete="NO ACTION"
                referencedColumnNames="id"
                referencedTableName="auto_transport"
                referencedTableCatalogName="concretebelarus"
                referencedTableSchemaName="concretebelarus"
        />
    </changeSet>
</databaseChangeLog>